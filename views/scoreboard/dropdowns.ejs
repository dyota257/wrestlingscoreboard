<td>
    <label>Style</label><br>
    <div style="text-align:left;">
        <% ['Freestyle', 'Greco-Roman'].forEach((e)=>{ %>
            <input type="radio" name="wrestlingStyle" onclick="setGameType('<%= e %>')" value="<%= e %>" id="wrestlingStyle<%= e %>"> 
            <label style="font-weight:normal;" for="wrestlingStyle<%= e %>"> <%= e %> </label><br>
        <% }) %>
    </div>
    <br>

    <label>Match time</label><br>
    <div style="text-align:left;">
        <% [2, 3].forEach((e)=>{ %>
            <input type="radio" name="matchTime" onclick="setMatchTime(<%= e %>)" value="<%= e %>" id="matchTime<%= e %>">
            <label style="font-weight:normal;" for="matchTime<%= e %>"> <%= e %> minutes</label><br>
        <% }) %>
    </div>
    <br>
    <button class="resetGame" onclick="setConfirmGame(matchTime)">âœ… Confirm</button>
    <br>
    <button class="resetGame" onclick="cancelConfirmGame()" style="max-width:5rem;">Cancel</button>
    
</td>

